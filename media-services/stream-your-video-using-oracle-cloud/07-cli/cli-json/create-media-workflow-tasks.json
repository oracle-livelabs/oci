[
      {
        "type": "getFiles",
        "version": 1,
        "key": "getFiles",
        "prerequisites": [],
        "parameters": {
          "taskParameters": [
            {
              "target": "${/input/objectName}",
              "namespaceName": "${/input/namespaceName}",
              "bucketName": "${/input/bucketName}",
              "objectName": "${/input/objectName}"
            }
          ]
        }
      },
      {
        "type": "transcode",
        "version": 1,
        "key": "transcode",
        "prerequisites": [
          "getFiles"
        ],
        "parameters": {
          "transcodeType": "standardTranscode",
          "standardTranscode": {
            "input": "${/getFiles/taskParameters/0/target}",
            "outputPrefix": "${/output/objectNameFilename}standardTranscode",
            "videoCodec": "h264",
            "audioCodec": "aac",
            "packaging": {
              "packageType": "hls",
              "segmentLength": 6
            },
            "ladder": [
              {
                "size": {
                  "height": 1080,
                  "resizeMethod": "scale"
                }
              },
              {
                "size": {
                  "height": 720,
                  "resizeMethod": "scale"
                }
              },
              {
                "size": {
                  "height": 480,
                  "resizeMethod": "scale"
                }
              },
              {
                "size": {
                  "height": 360,
                  "resizeMethod": "scale"
                }
              }
            ]
          }
        }
      },
      {
        "type": "thumbnail",
        "version": 1,
        "key": "thumbnail",
        "prerequisites": [
          "transcode"
        ],
        "parameters": {
          "thumbnails": {
            "input": "${/getFiles/taskParameters/0/target}",
            "frameSelectors": [
              {
                "namePrefix": "thumbnail",
                "format": "jpg",
                "sizes": [
                  {
                    "width": 390
                  },
                  {
                    "width": 196
                  }
                ]
              }
            ]
          }
        }
      },
      {
        "type": "putFiles",
        "version": 1,
        "key": "putFiles",
        "prerequisites": [
          "thumbnail"
        ],
        "parameters": {
          "taskParameters": [
            {
              "namespaceName": "${/output/namespaceName}",
              "bucketName": "${/output/bucketName}",
              "source": "*.m3u8",
              "objectName": "${/output/basePrefix}${/taskOutput/transcode}",
              "assetCompartmentId": "${/output/assetCompartmentId}",
              "registerMetadata": true
            },
            {
              "namespaceName": "${/output/namespaceName}",
              "bucketName": "${/output/bucketName}",
              "source": "*.fmp4",
              "objectName": "${/output/basePrefix}${/taskOutput/transcode}",
              "assetCompartmentId": "${/output/assetCompartmentId}",
              "registerMetadata": true
            },
            {
              "namespaceName": "${/output/namespaceName}",
              "bucketName": "${/output/bucketName}",
              "source": "*.${/thumbnail/thumbnails/frameSelectors/0/format}",
              "objectName": "${/output/basePrefix}${/taskOutput/thumbnail}${/output/objectNameFilename}",
              "assetCompartmentId": "${/output/assetCompartmentId}",
              "registerMetadata": true
            }
          ]
        }
      }
    ]